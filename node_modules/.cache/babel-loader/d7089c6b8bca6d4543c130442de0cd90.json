 {\r\n  name: \"songList\",\r\n  data() {\r\n    return {\r\n      tags: {},\r\n      highquality:{\r\n        coverImgUrl:\"\",\r\nname:'',\r\ncopywriter:''\r\n\r\n      },\r\n      songList: {\r\n        title: \"推荐歌单\",\r\n        list: [{ id: undefined, picUrl: \"\" }],\r\n      },\r\n    };\r\n  },\r\n  props: [\"limit\", \"isFull\"],\r\n  computed: {\r\n   \r\n  },\r\n  methods: {\r\n    handel(e){\r\n      console.log(e)\r\n     \r\n    },\r\n    \r\n  async  loadALl() {\r\n\r\n    await  getTags().then((response) => {\r\n        this.tags = response.data.tags.slice(0,9);\r\n       \r\n      });\r\n        \r\n      getHighquality({limit:1,cat: this.isFull?\"\":\"小语种\"}).then((response)=>{\r\n\r\nthis.highquality = response.data.playlists[0];\r\n      })\r\n      getList({limit:this.limit}).then((response) => {\r\n        let arr = response.data.playlists;\r\n        this.songList.list = arr.map((item) => {\r\n          return Object.assign({\r\n            id: item.id,\r\n            picUrl: item.coverImgUrl,\r\n            name: item.name,\r\n          });\r\n        });\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadALl();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.ad {\r\n  position: relative;\r\n  background-size: 21000px 21000px;\r\n  padding-top: 20px;\r\n  padding-left: 50px;\r\n  border-radius: 0.5em;\r\n  color: #fff;\r\n  font-size: 14px;\r\n}\r\n.dis {\r\n  padding-left: 18px;\r\n  margin-top: 45px;\r\n}\r\n.top {\r\n  color: #d3b301;\r\n  box-sizing: border-box;\r\n  border: 1px #d3b301 solid;\r\n  padding: 7px 12px;\r\n  border-radius: 1.5em;\r\n  cursor: pointer;\r\n}\r\n.tag {\r\n  position: absolute;\r\n  color: #25231d;\r\n  right: 20px;\r\n  bottom: -40px;\r\n}\r\n.active {\r\n  background: red;\r\n}\r\n.item {\r\n  margin-right: 10px;\r\n  font-size: 8px;\r\n  padding: 3px 9px;\r\n  border: 1px #5a7099 solid;\r\n  border-radius: 1.5em;\r\n  cursor:pointer;\r\n}\r\n.el-col {\r\n  margin-bottom: 20px;\r\n}\r\nh3 {\r\n  display: inline-block;\r\n  color: #5a7099;\r\n  padding-left: 7.5px;\r\n}\r\nimg {\r\n  width: 196px;\r\n  min-height: 196px;\r\n}\r\n.dog {\r\n  padding: 9px;\r\n  height: 35px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}