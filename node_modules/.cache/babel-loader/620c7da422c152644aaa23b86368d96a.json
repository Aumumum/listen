ourcesContent":["<template>\r\n  <el-autocomplete\r\n    v-model=\"state\"\r\n    :fetch-suggestions=\"querySearchAsync\"\r\n    placeholder=\"请输入内容\"\r\n    @select=\"handleSelect\"\r\n  ></el-autocomplete>\r\n</template>\r\n\r\n<script>\r\nimport {getSuggest} from '../../api'\r\nexport default {\r\n  name: \"search\",\r\n  data() {\r\n    return {\r\n      suggests:[],\r\n      state: \"\",\r\n      timeout: null,\r\n    };\r\n  },\r\n  methods: {\r\n    loadAll() {\r\n      getSuggest().then((response)=>{\r\n        this.suggests=response.data.result.hots.map((item)=>{\r\n          return Object.assign({'value':item.first}) \r\n        })\r\n      });\r\n    },\r\n    querySearchAsync(queryString, cb) {\r\n      var restaurants = this.restaurants;\r\n      var results = queryString\r\n        ? restaurants.filter(this.createStateFilter(queryString))\r\n        : this.suggests;\r\n\r\n        cb(results);\r\n    },\r\n    createStateFilter(queryString) {\r\n      return (state) => {\r\n        return (\r\n          state.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0\r\n        );\r\n      };\r\n    },\r\n    handleSelect(item) {\r\n      console.log(item);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadAll()\r\n     console.log(this.suggests)\r\n  },\r\n};\r\n</script>\r\n\r\n<style spoed>\r\n.el-autocomplete {\r\n  line-height: 62px;\r\n}\r\ninput[type=\"text\"] {\r\n  border-radius: 1.5em;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/top_bar"}]}