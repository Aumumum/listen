AA,IAAA,CAAA,SAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,GAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,iBAAA,MAAA,CAAA,MAAA,CAAA;AACA,YAAA,EAAA,EAAA,IAAA,CAAA,EADA;AAEA,YAAA,MAAA,EAAA,IAAA,CAAA,WAFA;AAGA,YAAA,IAAA,EAAA,IAAA,CAAA;AAHA,WAAA,CAAA;AAKA,SANA,CAAA;AAOA,OAbA;AAcA;AAlDA,GA1CA;AA8FA,EAAA,OA9FA,qBA8FA;AAAA;;AACA,IAAA,OAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,KAFA;AAGA;AAlGA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row\r\n      v-if=\"isFull\"\r\n      class=\"ad animate__animated animate__bounceInDown\"\r\n      :style=\"{ backgroundImage: 'url(' + highquality.coverImgUrl + ')' }\"\r\n    >\r\n      <el-col :span=\"4\">\r\n        <img :src=\"highquality.coverImgUrl\" lazy alt=\"精品歌单封面图片\" />\r\n      </el-col>\r\n      <el-col class=\"dis\" :span=\"20\">\r\n        <span class=\"top\" @click=\"songs(highquality.id)\">\r\n          <i class=\"el-icon-medal-1\" style=\"paddingRight:10px\" />精品歌单\r\n        </span>\r\n        <h1>{{ highquality.name }}</h1>\r\n        <span>{{ highquality.copywriter }}</span>\r\n      </el-col>\r\n      <div class=\"tag animate__animated  animate__fadeIn animate__delay-1s\">\r\n        <span v-for=\"(tag, index) in tags\" :key=\"index\">\r\n          <span\r\n            class=\"item \"\r\n            :class=\"{ active: index === tags.isActive }\"\r\n            @click=\"handel(index, tag.name)\"\r\n          >\r\n            {{ tag.name }}\r\n          </span>\r\n        </span>\r\n      </div>\r\n    </el-row>\r\n    <h3 class=\"animate__animated  animate__fadeIn animate__delay-1s\">\r\n      {{ songList.title }}\r\n    </h3>\r\n    <el-row :gutter=\"15\">\r\n      <el-col v-for=\"item in songList.list\" :key=\"item.id\" :span=\"4\">\r\n        <el-card\r\n          class=\"animate__animated animate__flipInX\"\r\n          shadow=\"hover\"\r\n          :body-style=\"{ padding: '0px' }\"\r\n        >\r\n          <img @click=\"songs(item.id)\" :src=\"item.picUrl\" lazy alt=\"歌单封面\" />\r\n          <div class=\"dog\">\r\n            <span @click=\"songs(item.id)\">{{ item.name }}</span>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <div\r\n      v-show=\"isFull\"\r\n      class=\"block animate__animated  animate__fadeIn animate__delay-1s\"\r\n    >\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :page-sizes=\"[18, 30, 42, 60]\"\r\n        :page-size=\"18\"\r\n        :current-page=\"songList.pagination\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"1200\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getList, getTags, getHighquality, getSongs } from \"../api/index\";\r\nexport default {\r\n  name: \"songList\",\r\n  data() {\r\n    return {\r\n      tags: { isActive: -1 },\r\n      highquality: {\r\n        coverImgUrl: \"\",\r\n        name: \"\",\r\n        copywriter: \"\",\r\n        id: \"\",\r\n      },\r\n      songList: { limit: 18, pagination: 1, title: \"推荐歌单\", list: [] },\r\n    };\r\n  },\r\n  props: [\"isFull\"],\r\n  computed: {\r\n    offset() {\r\n      return (this.songList.pagination - 1) * this.songList.limit;\r\n    },\r\n    cat() {\r\n      return this.songList.title === \"推荐歌单\" ? \"全部\" : this.songList.title;\r\n    },\r\n    stateChange() {\r\n      const { limit, pagination, title } = this.songList;\r\n      return { limit, pagination, title };\r\n    },\r\n  },\r\n  watch: {\r\n    \"songList.title\": {\r\n      async handler() {\r\n        await this.loadALl();\r\n      },\r\n      immediate: true,\r\n    },\r\n    stateChange: {\r\n      handler(ne) {\r\n        console.log(ne);\r\n        this.listChange();\r\n      },\r\n      immediate: true,\r\n    },\r\n  },\r\n  methods: {\r\n    handel(index, name) {\r\n      this.tags.isActive = index;\r\n      this.songList.title = name;\r\n      this.songList.pagination = 1;\r\n    },\r\n    handleSizeChange(val) {\r\n      this.songList.limit = val;\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.songList.pagination = val;\r\n    },\r\n    songs(id) {\r\n      getSongs({ id }).then((response) => {\r\n        this.$router.push({\r\n          path: \"/newest\",\r\n          query: {\r\n            q: \"list\",\r\n            id,\r\n          },\r\n        });\r\n        this.$store.state.trackIds = response.data.playlist.trackIds;\r\n      });\r\n    },\r\n    loadALl() {\r\n      getHighquality({\r\n        limit: 1,\r\n        cat: this.cat,\r\n      }).then((response) => {\r\n        let obj = response.data.playlists[0];\r\n        Object.keys(this.highquality).forEach((key) => {\r\n          this.highquality[key] = obj[key];\r\n        });\r\n      });\r\n    },\r\n    listChange() {\r\n      getList({\r\n        limit: this.isFull ? this.songList.limit : 12,\r\n        offset: this.offset,\r\n        cat: this.cat,\r\n      }).then((response) => {\r\n        let arr = response.data.playlists;\r\n        this.songList.list = arr.map((item) => {\r\n          return Object.assign({\r\n            id: item.id,\r\n            picUrl: item.coverImgUrl,\r\n            name: item.name,\r\n          });\r\n        });\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    getTags().then((response) => {\r\n      this.tags = response.data.tags.slice(0, 9);\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.ad {\r\n  position: relative;\r\n  background-size: 21000px 21000px;\r\n  padding-top: 20px;\r\n  padding-left: 50px;\r\n  border-radius: 0.5em;\r\n  color: #fff;\r\n  font-size: 14px;\r\n}\r\n.dis {\r\n  padding-left: 28px;\r\n  margin-top: 45px;\r\n}\r\n.top {\r\n  color: #d3b301;\r\n  box-sizing: border-box;\r\n  border: 1px #d3b301 solid;\r\n  padding: 7px 12px;\r\n  border-radius: 1.5em;\r\n  cursor: pointer;\r\n}\r\n.tag {\r\n  position: absolute;\r\n  color: #25231d;\r\n  right: 20px;\r\n  bottom: -40px;\r\n}\r\n.active {\r\n  background: red;\r\n}\r\n.item {\r\n  margin-right: 10px;\r\n  font-size: 8px;\r\n  padding: 3px 9px;\r\n  border: 1px #5a7099 solid;\r\n  border-radius: 1.5em;\r\n  cursor: pointer;\r\n}\r\n.el-col {\r\n  margin-bottom: 20px;\r\n}\r\nh3 {\r\n  display: inline-block;\r\n  color: #5a7099;\r\n  padding-left: 7.5px;\r\n}\r\nimg {\r\n  cursor: pointer;\r\n  width: 196px;\r\n  min-height: 196px;\r\n}\r\n.dog {\r\n  cursor: pointer;\r\n  overflow: visible;\r\n  padding: 9px;\r\n  height: 35px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}